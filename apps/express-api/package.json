{
  "name": "express-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc --project tsconfig.build.json",
    "start": "pnpm run start",
    "db:introspect": "kysely-codegen --out-file ./src/types/database.d.ts --runtime-enums=screaming-snake-case",
    "db:migrate-latest": "tsc --project tsconfig.build.json && node dist/database/migrate-latest.js && pnpm run db:introspect",
    "db:migrate-up": "tsc --project tsconfig.build.json && node dist/database/migrate-up.js && pnpm run db:introspect",
    "db:migrate-down": "tsc --project tsconfig.build.json && node dist/database/migrate-down.js && pnpm run db:introspect",
    "db:generate-migration": "./scripts/newMigrationFile.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "kysely": "^0.28.11",
    "morgan": "^1.10.1",
    "pg": "^8.18.0",
    "tsx": "^4.21.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/pg": "^8.16.0",
    "kysely-codegen": "^0.19.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "turbo": "^2.8.3"
  }
}
